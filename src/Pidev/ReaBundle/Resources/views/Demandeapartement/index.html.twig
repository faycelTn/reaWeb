{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Demandeapartement list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Nbrpiece</th>
                <th>Etage</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('demandeapartement_show', { 'id': entity.iddemande }) }}">{{ entity.nbrpiece }}</a></td>
                <td>{{ entity.etage }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('demandeapartement_show', { 'id': entity.iddemande }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('demandeapartement_edit', { 'id': entity.iddemande }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('demandeapartement_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
